---
#
# Ansible managed
#
exclude_paths:
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/verify.yml
  - molecule/default/collections.yml
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]
  - name[casing]
  - var-naming[no-role-prefix]
  - yaml[line-length]

enable_list:
  - name[prefix]
